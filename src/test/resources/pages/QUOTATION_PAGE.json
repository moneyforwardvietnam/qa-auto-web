{
  "name": "QUOTATION_PAGE",
  "detectors": {
    "web": "QUOTATION_PAGE_TITLE_LABEL"
  },
  "elements": [
    {
      "name": "QUOTATION_PAGE_TITLE_LABEL",
      "locators": {
        "web": {
          "selector": "//div[@class='title-bar__title' and (contains(text(),'Quotation') or contains(text(),'見積書'))]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_CREATE_BUTTON",
      "locators": {
        "web": {
          "selector": "//button[@class='el-button el-button--primary']",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_CLIENT_NAME_INPUT",
      "locators": {
        "web": {
          "selector": "//div[@class='quotation-client-name-input']//child::input[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_ERROR_MESSAGE_LABEL",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Please input client')]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_CLIENT_DETAILS_INPUT",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Client detail')]//following::textarea[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_TITLE_INPUT",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Title')]//following::input[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_QUOTATION_NUMBER_INPUT",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Quotation number')]//following::input[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_QUOTE_DATE_INPUT",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Quote date')]//following::input[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_EXPIRATION_DATE_INPUT",
      "locators": {
        "web": {
          "selector": "//div[contains(text(),'Expiration date')]//following::input[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_FIRST_ITEM_NAME_INPUT",
      "locators": {
        "web": {
          "selector": "(//div[@class='cell' and contains(text(),'品目')]//following::div[@class='el-dropdown'])[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_SAVED_SUCCESS_LABEL",
      "locators": {
        "web": {
          "selector": "//p[text()='見積書を作成しました。']//parent::div[@class='el-message el-message--success is-closable']",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "QUOTATION_PAGE_NEW_QUOTATION_ISSUER_PREVIEW_LABEL",
      "locators": {
        "web": {
          "selector": "(//div[@class='publisher'])[2]//child::span[1]",
          "strategy": "xpath"
        }
      }
    }
  ]
}