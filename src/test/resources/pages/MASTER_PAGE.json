{
  "name": "MASTER_PAGE",
  "detectors": {
    "web": "MASTER_PAGE_LEFT_MENU"
  },
  "elements": [
    {
      "name": "MASTER_PAGE_LEFT_MENU",
      "locators": {
        "web": {
          "selector": "//ul[@class='el-menu']",
          "strategy": "xpath"
        }
      }
    },{
      "name": "MASTER_PAGE_LEFT_MENU_SETTINGS_LABEL",
      "locators": {
        "web": {
          "selector": "//ul[@class='el-menu']//child::span[contains(text(),'設定')]//parent::li[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "MASTER_PAGE_LANGUAGE_SETTING_DROPDOWN",
      "locators": {
        "web": {
          "selector": "//span[@class='language__label']//parent::div[1]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "MASTER_PAGE_LANGUAGE_SETTING_JAPANESE_ITEM",
      "locators": {
        "web": {
          "selector": "//li[@class='el-dropdown-menu__item option' and contains(text(),'日本語')]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "MASTER_PAGE_LANGUAGE_SETTING_ENGLISH_ITEM",
      "locators": {
        "web": {
          "selector": "//li[@class='el-dropdown-menu__item option' and contains(text(),'English')]",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "MASTER_PAGE_LOGGED_IN_USER_LABEL",
      "locators": {
        "web": {
          "selector": "//span[@class='user__name']",
          "strategy": "xpath"
        }
      }
    },
    {
      "name": "MASTER_PAGE_LOGOUT_BUTTON",
      "locators": {
        "web": {
          "selector": "//li[contains(text(),'ログアウト')]",
          "strategy": "xpath"
        }
      }
    }
  ]
}